# Frontend Environment Variables (exposed to browser - SAFE to expose)
REACT_APP_SUPABASE_URL=your-supabase-url
REACT_APP_SUPABASE_ANON_KEY=your-anon-key

# Server-side Environment Variables (NOT exposed to browser)
# Note: SUPABASE_URL and SUPABASE_ANON_KEY should have the same values as above
# They're duplicated because server-side code can't access REACT_APP_ variables
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# API Keys (Server-side only - NEVER expose to browser)
GEMINI_API_KEY=your-gemini-key
TINYMCE_API_KEY=your-tinymce-key

# Server Configuration
PORT=5000